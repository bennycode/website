{
  "dependencies": {
    "dotenv": "4.0.0",
    "express": "4.15.4",
    "nodemon": "1.11.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "concurrently": "3.5.0",
    "cross-env": "5.0.5",
    "istanbul": "0.4.5",
    "jasmine": "2.7.0",
    "opn-cli": "3.1.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "request": "2.81.0",
    "rimraf": "2.6.1",
    "webpack": "3.5.5"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "index.js",
  "name": "@welovecoding/website",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welovecoding/website.git"
  },
  "scripts": {
    "coverage:backend": "rimraf coverage && npm run dist && cross-env JASMINE_CONFIG_PATH=src/test/support/jasmine.json istanbul cover --report html ./node_modules/jasmine/bin/jasmine.js",
    "dist": "npm run dist:frontend",
    "dist:frontend": "webpack",
    "preversion": "npm install && npm run dist && npm test",
    "version": "npm run dist",
    "postversion": "git push && git push --tags",
    "start": "npm run dist && concurrently \"nodemon index.js\" \"opn http://localhost:8080/\"",
    "test": "npm run dist && npm run test:backend",
    "test:backend": "cross-env JASMINE_CONFIG_PATH=src/test/support/jasmine.json jasmine"
  },
  "version": "1.0.11"
}
